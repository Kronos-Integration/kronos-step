{
  "name": "kronos-step",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/step.js",
  "module": "src/step.js",
  "description": "basic step implementation",
  "keywords": [],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "docs": "documentation readme src/step.js --section=API",
    "lint": "documentation lint src/step.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "kronos-service": "^4.17.2",
    "loglevel-mixin": "^3.0.0"
  },
  "devDependencies": {
    "@kronos-integration/test-step": "^5.0.0",
    "ava": "^1.3.1",
    "c8": "^3.4.0",
    "documentation": "^9.3.1",
    "esm": "^3.2.18",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.6.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-executable": "^1.4.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-step.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-step/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-step#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
